{
    "name": "resume",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "prepare": "husky install",
        "dev": "VITE_COMMIT_REF=$(git show -s --format=%h) vite --host",
        "build": "export VITE_COMMIT_REF=$(git show -s --format=%h) && vite build --outDir dist/static && vite build --ssr src/server-render.tsx --outDir dist/server && node prerender",
        "prebuild": "rm -rf dist/static",
        "preview": "vite preview",
        "lint": "eslint -c .eslintrc.json --ext .ts ./src"
    },
    "dependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@types/debounce": "^1.2.1",
        "@types/react": "^17.0.33",
        "@types/react-dom": "^17.0.10",
        "@types/styled-components": "^5.1.23",
        "@typescript-eslint/parser": "^5.56.0",
        "@vitejs/plugin-react": "^1.0.7",
        "axios": "^0.26.0",
        "debounce": "^1.2.1",
        "eslint": "^8.36.0",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-v3xlabs": "^1.6.0",
        "husky": "^8.0.3",
        "prettier": "^2.8.7",
        "react-feather": "^2.0.9",
        "styled-components": "^5.3.3",
        "typescript": "^4.9.5",
        "vite": "^2.8.0",
        "vite-plugin-compression": "^0.5.1"
    }
}
