{
  "name": "resume",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "VITE_COMMIT_REF=$(git show -s --format=%h) vite",
    "build": "export VITE_COMMIT_REF=$(git show -s --format=%h) && vite build --outDir dist/static && vite build --ssr src/server-render.tsx --outDir dist/server && node prerender",
    "prebuild": "rm -rf dist/static",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/styled-components": "^5.1.23",
    "@vitejs/plugin-react": "^1.0.7",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vite-plugin-compression": "^0.5.1"
  }
}
